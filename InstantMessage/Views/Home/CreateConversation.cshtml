@model IEnumerable<InstantMessage.Models.User>
@{
    ViewBag.Title = "CreateConversation";
}

<h2>CreateConversation</h2>

<h3>Select User</h3>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserID)
            </td>
            <td>
                @Html.ActionLink("Select", "Conversation",new {id = item.UserID })
            </td>
           
        </tr>

    }
</table>

